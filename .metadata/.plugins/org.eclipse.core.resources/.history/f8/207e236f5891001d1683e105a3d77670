package spring.tv;

public class LgTv implements TV {
	
	public void powerOn() {
		System.out.println("let's go lg");
	}
	public void soundUp(int v) {
		System.out.println("" + v++);
	}
	public void soundDown(int v) {
		System.out.println("" + v++);
	}
	public void powerOff() {
		System.out.println("bye lg");
	}


}
